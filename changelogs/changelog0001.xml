            <column name="title_level" type="text" defaultValue="" remarks=""><constraints nullable="false"/></column>
            <column name="removed_date" type="timestamp" remarks="date and time of insertion given row to database" />
            <column name="removed_by" type="text" remarks="account_uid of remover of that row" />
            <column name="is_active" type="bigint" defaultValueNumeric="1" remarks="If this row is still active (value=1) or has been removed (value=0)"><constraints nullable="false"/></column>
            <column name="created_date" type="timestamp" defaultValueDate="now()" remarks="date and time of insertion given row to database" />
            <column name="last_updated_date" type="timestamp" defaultValueDate="now()" remarks="date and time of change this row" />
            <column name="created_by" type="text" defaultValue="system"><constraints nullable="false"/></column>
            <column name="last_updated_by" type="text" defaultValue="system"><constraints nullable="false"/></column>
            <column name="custom_attributes" type="text" defaultValue="{}"><constraints nullable="false"/></column>
