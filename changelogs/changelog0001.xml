            <column name="created_by" type="text" defaultValue="system"><constraints nullable="false"/></column>
            <column name="last_updated_by" type="text" defaultValue="system"><constraints nullable="false"/></column>
            <column name="custom_attributes" type="text" defaultValue="{}"><constraints nullable="false"/></column>
